{
    "name": "Slack/Discord Notifications",
    "author": "VANet",
    "author-gh": "va-net",
    "author-site": "https://vanet.app",
    "class": "NotifyPlugin",
    "description": "Plugin to add Slack or Discord Notifications",
    "tags": ["slack", "discord", "notifications", "official"],
    "version": {
        "tag": "1.0.0",
        "name": "Version 1.0.0",
        "date": "2020-10-26"
    },
    "compatability": ["v1.0.0"],
    "installation": {
        "files": [
            "classes/NotifyPlugin.php",
            "admin/notify_plugin.php"
        ],
        "queries": [
            "INSERT INTO `options` (`name`, `value`) VALUES ('DISCORD_WEBHOOK', ''), ('DISCORD_WEBHOOK_PRIVATE', ''), ('SLACK_WEBHOOK', ''), ('SLACK_WEBHOOK_PRIVATE', '');"
        ]
    }
}